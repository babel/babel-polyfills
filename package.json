{
  "name": "babel-polyfills",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "gulp build",
    "clean": "rimraf packages/*/lib",
    "clean-all": "rimraf packages/*/lib packages/*/node_modules node_modules",
    "flow": "flow check --strip-root",
    "lint": "eslint packages '*.js' --format=codeframe",
    "postinstall": "lerna bootstrap",
    "test": "jest",
    "watch": "gulp watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/eslint-plugin-development": "^1.0.1",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-babel": "^9.0.0",
    "eslint-plugin-flowtype": "^3.8.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.1.0",
    "flow-bin": "^0.94.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-newer": "^1.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-uglify": "^3.0.2",
    "gulp-watch": "^5.0.1",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lerna": "^3.14.1",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "through2": "^2.0.0"
  },
  "engines": {
    "node": ">= 6.9.0",
    "yarn": ">=1.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --format=codeframe"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testRegex": "./packages/[^/]+/test/.+\\.m?js$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/",
      "/test/helpers/"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/",
      "<rootDir>/packages/[^/]+/lib/"
    ],
    "modulePathIgnorePatterns": [
      "/test/fixtures/"
    ],
    "moduleNameMapper": {
      "^@babel/plugin-inject-polyfills$": "<rootDir>/packages/babel-plugin-inject-polyfills/"
    }
  },
  "version": "0.0.0"
}
