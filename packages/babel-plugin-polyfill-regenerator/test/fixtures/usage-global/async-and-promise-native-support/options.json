{
  "externalHelpers": false,
  "plugins": [
    [
      "@@/polyfill-regenerator",
      {
        "method": "usage-global"
      }
    ],
    [
      "@@/polyfill-corejs2",
      {
        "method": "usage-global",
        "targets": {
          "chrome": 70
        }
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "chrome": 70
        }
      }
    ]
  ]
}
