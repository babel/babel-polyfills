{
  "plugins": [
    [
      "@babel/plugin-inject-polyfills",
      {
        "method": "entry-global",
        "targets": { "chrome": 55 },
        "providers": [
          [
            "@@/corejs2",
            {
              "exclude": ["es6.string.repeat"],
              "include": ["es6.map"]
            }
          ]
        ]
      }
    ]
  ]
}
