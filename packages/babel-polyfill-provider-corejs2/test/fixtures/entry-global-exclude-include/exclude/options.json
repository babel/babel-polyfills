{
  "plugins": [
    [
      "@babel/plugin-inject-polyfills",
      {
        "method": "entry-global",
        "targets": { "chrome": 63 },
        "providers": [
          [
            "../../../../../../packages/babel-polyfill-provider-corejs2",
            {
              "exclude": [
                "es7.array.flat-map",
                "web.timers",
                "web.immediate",
                "web.dom.iterable"
              ]
            }
          ]
        ]
      }
    ]
  ]
}
