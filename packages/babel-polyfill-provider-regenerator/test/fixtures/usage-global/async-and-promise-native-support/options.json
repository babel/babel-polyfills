{
  "plugins": [
    [
      "@babel/plugin-inject-polyfills",
      {
        "method": "usage-global",
        "targets": { "chrome": 70 },
        "providers": [
          "@@/babel-polyfill-provider-regenerator",
          "@@/babel-polyfill-provider-corejs2"
        ]
      }
    ]
  ],
  "presets": [
    ["@babel/preset-env", { "modules": false, "targets": { "chrome": 70 } }]
  ]
}
