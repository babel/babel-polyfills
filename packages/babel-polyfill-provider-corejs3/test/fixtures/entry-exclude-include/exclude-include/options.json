{
  "plugins": [
    [
      "@babel/plugin-inject-polyfills",
      {
        "method": "entry-global",
        "targets": { "chrome": 55 },
        "providers": [
          [
            "module:./packages/babel-polyfill-provider-corejs3",
            {
              "exclude": ["es.string.pad-start"],
              "include": ["es.map"]
            }
          ]
        ]
      }
    ]
  ]
}
