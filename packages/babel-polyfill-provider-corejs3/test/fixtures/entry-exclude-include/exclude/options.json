{
  "plugins": [
    [
      "@babel/plugin-inject-polyfills",
      {
        "method": "entry-global",
        "targets": { "chrome": 63 },
        "providers": [
          [
            "../../../../../../packages/babel-polyfill-provider-corejs3",
            {
              "exclude": [
                "es.string.pad-start",
                "es.string.pad-end",
                "web.timers",
                "web.immediate",
                "web.dom-collections.iterator"
              ]
            }
          ]
        ]
      }
    ]
  ]
}
